<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".NoiQuy"
    android:background="@drawable/background_setting">
    <CheckBox
        android:id="@+id/CBEffect"
        android:layout_width="183dp"
        android:layout_height="37dp"
        android:layout_marginBottom="20dp"
        android:layout_marginEnd="95dp"
        android:layout_marginStart="82dp"
        android:layout_marginTop="40dp"
        android:checked="true"
        android:text="Hiệu ứng"
        android:textColor="#5C1515"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CBMusic" />

    <CheckBox
        android:id="@+id/CBMusic"
        android:layout_width="183dp"
        android:layout_height="37dp"
        android:layout_marginStart="82dp"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="95dp"
        android:layout_marginBottom="23dp"
        android:checked="true"
        android:text="Nhạc nền"
        android:textColor="#611F1F"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/CBEffect"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>